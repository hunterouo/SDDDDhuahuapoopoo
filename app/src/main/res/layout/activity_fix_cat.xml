<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.wang.huntergod.catinforFragment"
    android:background="@drawable/loginpage"
    android:weightSum="1">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="164dp"
        android:layout_height="match_parent">


        <Space
            android:layout_width="match_parent"
            android:layout_height="50dp" />
        <ImageView
            android:layout_width="120sp"
            android:layout_height="120sp"
            android:id="@+id/imageView1"
            android:background="@drawable/cat001"
            android:layout_gravity="center_horizontal" />
        <Space
            android:layout_width="match_parent"
            android:layout_height="60dp" />
        <ImageView
            android:layout_width="120sp"
            android:layout_height="120sp"
            android:id="@+id/imageView2"
            android:background="@drawable/cat002"
            android:layout_gravity="center_horizontal" />
        <Space
            android:layout_width="match_parent"
            android:layout_height="50dp" />

    </LinearLayout>

    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout

            android:orientation="vertical"
            android:layout_width="225dp"
            android:layout_height="wrap_content"
            android:id="@+id/relativeLayout">
            <Space
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:id="@+id/space8" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:text="完成"
                android:id="@+id/button9"
                style="@style/RDButton"
                android:layout_alignBottom="@+id/space8"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

        </RelativeLayout>

        <Space
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:layout_below="@+id/relativeLayout"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/space" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="名字："
            android:id="@+id/textView10"
            android:layout_below="@+id/space"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="大頭"
            android:id="@+id/textView11"
            android:layout_below="@+id/relativeLayout"
            android:layout_toRightOf="@+id/textView10"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <Space
            android:layout_width="20px"
            android:layout_height="20px"
            android:layout_below="@+id/textView10"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/space9" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="性別："
            android:id="@+id/textView12"
            android:layout_below="@+id/space9"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="母"
            android:id="@+id/textView13"
            android:layout_below="@+id/textView10"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_toRightOf="@+id/textView10"
            android:layout_toEndOf="@+id/textView10" />

        <Space
            android:layout_width="20px"
            android:layout_height="20px"
            android:layout_below="@+id/textView12"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/space10" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="領養："
            android:id="@+id/textView14"
            android:layout_below="@+id/textView13"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />
        <Space
            android:layout_width="fill_parent"
            android:layout_height="120px"
            android:layout_below="@+id/textView14"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignRight="@+id/textView14"
            android:layout_alignEnd="@+id/textView14"
            android:id="@+id/space3"
            />
        <Space
            android:layout_width="match_parent"
            android:layout_height="10dp"
            android:layout_below="@+id/space3"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/space4" />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="名字："
            android:id="@+id/textView16"
            android:layout_below="@+id/space4"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="莉娜"
            android:id="@+id/textView17"
            android:layout_alignTop="@+id/space4"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_toRightOf="@+id/textView16"
            android:layout_toEndOf="@+id/textView16" />

        <Space
            android:layout_width="20px"
            android:layout_height="20px"
            android:layout_below="@+id/textView16"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/space5" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="性別："
            android:id="@+id/textView18"
            android:layout_below="@+id/space5"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="母"
            android:id="@+id/textView19"
            android:layout_below="@+id/textView16"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            android:layout_toRightOf="@+id/textView16"
            android:layout_toEndOf="@+id/textView16" />

        <Space
            android:layout_width="20px"
            android:layout_height="20px"
            android:layout_below="@+id/textView19"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/space6" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="領養："
            android:id="@+id/textView20"
            android:layout_below="@+id/textView19"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <Space
            android:layout_width="fill_parent"
            android:layout_height="125px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/space0"
            android:layout_alignTop="@+id/textView20" />




        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner1"
            android:layout_below="@+id/textView13"
            android:layout_toEndOf="@+id/textView14"
            android:layout_above="@+id/space3"
            android:layout_toRightOf="@+id/textView14" />

        <Spinner
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/spinner2"
            android:layout_below="@+id/textView19"
            android:layout_toRightOf="@+id/textView20"
            android:layout_toEndOf="@+id/textView20"
            android:layout_alignBottom="@+id/textView20" />



    </RelativeLayout>


</LinearLayout>
